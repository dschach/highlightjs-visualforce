	"version": "1.0.0",
	"description": "Visualforce Markup of Salesforce highlighting plugin for highlight.js",
	"main": "src/languages/visualforce.js",
	"scripts": {
		"test": "mocha --reporter spec",
		"mocha": "mocha test/markup",
		"npm-patch": "npm version patch && npm publish",
