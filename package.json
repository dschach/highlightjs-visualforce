		"prettier:verify": "prettier --list-different \"{test,src,demo,scripts}/**/*\"",
		"prettier": "prettier --write \"{test,src,demo,scripts}/**/*\""
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/dschach/highlightjs-visualforce.git"
